      "type": "string"
    },
    "data": {
      "type": [
        "string",
        "null"
      ]
    },
    "description": {
      "type": [
        "string",
        "null"
      ]
    },
    "extensible": {
      "type": "boolean"
    },
    "nsfw": {
      "type": "boolean"
    },
    "preview_uri": {
      "type": [
        "string",
