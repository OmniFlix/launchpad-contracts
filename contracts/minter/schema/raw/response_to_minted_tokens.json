{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "UserDetails",
  "type": "object",
  "required": [
    "minted_tokens",
    "rounds_mints",
    "total_minted_count"
  ],
  "properties": {
    "minted_tokens": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Token"
      }
    },
    "rounds_mints": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/MintCountInRound"
      }
    },
    "total_minted_count": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    }
  },
  "additionalProperties": false,
  "definitions": {
    "MintCountInRound": {
      "type": "object",
      "required": [
        "count",
        "round_index"
      ],
      "properties": {
        "count": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        },
        "round_index": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        }
      },
      "additionalProperties": false
    },
    "Token": {
      "type": "object",
      "required": [
        "token_id"
      ],
      "properties": {
        "token_id": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  }
}
